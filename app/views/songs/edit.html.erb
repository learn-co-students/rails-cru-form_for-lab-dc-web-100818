<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <%= form_for @song do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :artist_id %>
      <%= f.text_field :artist_id %>
      <%= f.label :genre_id %>
      <%= f.text_field :genre_id %>

      <%= f.submit "Update Song" %>
    <% end %>
  </body>
</html>
